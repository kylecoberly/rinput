{
    "manifest_version": 2,

    "name": "Rinput",
    "description": "This plugin allows you to collect user input on any site",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}
